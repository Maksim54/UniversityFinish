@model IEnumerable<University.ViewModels.EnrollmentDateGroup>

@{
	ViewBag.Title = "Student Body Statistics";
}

<h2>Üliõpilaskonna statistika </h2>

<table>
	<tr>
        <th>
            Registreerimise kuupäev
        </th>
        <th>
            Õpilased
        </th>
	</tr>

	@foreach (var item in Model)
	{
		<tr>
			<td>
				@Html.DisplayFor(modelItem => item.EnrollmentDate)
			</td>
			<td>
				@item.StudentCount
			</td>
		</tr>
	}
</table>